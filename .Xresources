!! ~/.Xresources

!!
!! rxvt-unicode (urxvt)
!!

!URxvt.termName:	urxvt

!URxvt.geometry:	96x32

!URxvt.scrollBar:		false
URxvt.scrollBar_right:		true
URxvt.scrollBar_floating:	true
URxvt.scrollstyle:		plain

URxvt.cursorBlink:	true
URxvt.cursorUnderline:	true
URxvt.pointerBlank:	true
URxvt.visualBell:	true

URxvt.saveLines:	2024
URxvt.fading:		40

!! Font linst and Spacing
URxvt.font:		xft:Ricty:size=10:antialias=true

URxvt.letterSpace:	-1
!URxvt.lineSpace:	0


!! Color scheme and Opacity
!! Opacity
!URxvt.depth:		32
URxvt*.transparent:	true
URxvt*.shading:		20

!! Colors - gruvbox-dark http://github.com/morhetz/gruvbox (a little customized ...)
